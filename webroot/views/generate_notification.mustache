
{{>header}}

<h3>Send push notification test</h3>

<form action="send" method="post">

    <input name="id" type="hidden" value="{{id}}"/>
    <div class="form-group">
      <label for="tokens">iOS device token (use comma to muliple tokens):</label>
      <input name="tokens" class="form-control" type="text" value="33F2BD6F3369F993BBF38A4F69B3102E3CCF4A5863D7F4B3355030585A3AD32C" />
    </div>
    <div class="form-group">
      <label for="registrationIDs">Android registration ID (use comma to muliple ids):</label>
      <input name="registrationIDs"  class="form-control" type="text" value=""/>
    </div>
    <div class="form-group">
      <label for="title">Title:</label>
      <input name="title" class="form-control" type="text" value="test"/>
    </div>
    <div class="form-group">
      <label for="text">Text:</label>
      <input name="text" class="form-control" type="text" value="text is here"/>
    </div>

    <p><input type="submit" class="btn btn-primary" value="Send"></p>

</form>

<form action="index" method="get">
  <input type="submit" class="btn btn-primary" value="Cancel">
</form>

{{>footer}}
